    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "serie1 = pd.Series([1,2,3,4,5])\n",
    "\n",
    "serie2 = pd.Series([2,4,6,8,10])\n",
    "\n",
    "union = pd.Series(np.union1d(serie1,serie2))\n",
    "\n",
    "intersectar = pd.Series(np.intersect1d(serie1, serie2))\n",
    "\n",
    "serie3 = union[~union.isin(intersectar)]\n",
    "\n",
    "serie3"
   ]
